# TeamVenture AI Service 环境变量配置模板
# 复制此文件为 .env 并填入真实值

# 应用配置
APP_NAME=teamventure-ai-service
DEBUG=true
HOST=0.0.0.0
PORT=8000

# OpenAI配置
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4-0125-preview
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=4000

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis123456
REDIS_DB=0

# RabbitMQ配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=admin123456
RABBITMQ_VHOST=/

# MQ Exchange/Queue配置
MQ_EXCHANGE=plan.generation.topic
MQ_QUEUE=ai.gen.req.queue
MQ_ROUTING_KEY=plan.request.#

# Java服务回调配置
JAVA_CALLBACK_URL=http://localhost:8080/internal/plans/batch
JAVA_INTERNAL_SECRET=change-this-in-production

# 日志配置
LOG_LEVEL=INFO
